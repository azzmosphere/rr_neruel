    "version": "0.2.0",
    "configurations": [

        {
            "name": "(lldb) Launch agent",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/main/rr-agent/rr-agent",
            "args": [
                "-t ${workspaceFolder}/resources/training/training-set.0.json",
                "-c ${workspaceFolder}/resources/etc/config.yaml"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "(lldb) Launch env",
            "type": "cppdbg",
            "request": "launch",
            "program": "sudo ${workspaceFolder}/build/src/main/rr-environment/rr-environment",
            "args": [
                "-c ${workspaceFolder}/resources/etc/config.yaml"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "(gdb) Launch env remote",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/main/rr-environment/rr-environment",
            "args": [
                "-c ${workspaceFolder}/resources/etc/config.yaml"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/home/aaron/bin/gdbscript"
        },
        {
            "name": "(lldb) NN Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/rr_nn_test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}